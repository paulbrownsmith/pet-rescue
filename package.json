  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "@mui/material-nextjs": "^7.3.5",
    "@mui/x-data-grid": "^8.19.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "leaflet": "^1.9.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-leaflet|@react-leaflet)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^leaflet$": "<rootDir>/node_modules/leaflet/dist/leaflet-src.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^10.1.0",
    "@storybook/addon-docs": "^10.1.0",
    "@storybook/addon-onboarding": "^10.1.0",
    "@storybook/preset-create-react-app": "^10.1.0",
    "@storybook/react-webpack5": "^10.1.0",
    "@vitest/mocker": "^4.0.14",
    "eslint-plugin-storybook": "^10.1.0",
    "identity-obj-proxy": "^3.0.0",
    "prop-types": "^15.8.1",
    "storybook": "^10.1.0",
    "webpack": "^5.103.0"
  }
}
